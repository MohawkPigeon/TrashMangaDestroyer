{
    "content_scripts": [
      {
        "js": ["manganato.js"],
        "matches": ["https://manganato.com/*"]
      }
    ],
    "manifest_version": 2,
    "name": "TrashMangaDestroyer",
    "version": "1.0.0",
  

    "permissions": [
        "activeTab",
        "<all_urls>"
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com object-src 'self'"
  }
  